{
  "name": "carp-vscode",
  "displayName": "Carp VS Code",
  "description": "Carp syntax highlighting",
  "publisher": "oliverfencott",
  "version": "0.1.4",
  "icon": "icon.png",
  "main": "./out/extension.js",
  "activationEvents": [
    "onLanguage:carp"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oliverfencott/carp-vscode"
  },
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "commands": [
      {
        "command": "carp-vscode.helloWorld",
        "title": "Say Hello"
      }
    ],
    "languages": [
      {
        "id": "carp",
        "aliases": [
          "Carp",
          "carp"
        ],
        "extensions": [
          ".carp"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "carp",
        "scopeName": "source.carp",
        "path": "./syntaxes/carp.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "carp",
        "path": "./snippets.json"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^17.0.19",
    "@vscode/test-electron": "^2.1.3",
    "typescript": "^4.6.2",
    "vsce": "^2.6.7",
    "vscode": "^1.1.37",
    "vscode-languageclient": "^7.0.0",
    "vscode-languageserver": "^7.0.0",
    "vscode-languageserver-textdocument": "^1.0.4"
  },
  "scripts": {
    "watch": "tsc --watch",
    "test": "echo 'Write some tests!'",
    "deploy": "vsce publish --yarn"
  }
}
